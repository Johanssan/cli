{
  "name": "@shoutem/cli",
  "version": "0.8.77",
  "description": "Command-line tools for Shoutem applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/cli"
  },
  "bin": {
    "shoutem": "./build/src/shoutem.js"
  },
  "scripts": {
    "build": "npm run clean && babel ./ --out-dir ./build --plugins transform-object-rest-spread --presets es2015,es2017 --source-maps --copy-files --ignore ./node_modules,./build/,./templates/",
    "clean": "rimraf ./build",
    "lint": "eslint ./ --ignore-pattern build/",
    "test": "mocha -R spec --compilers js:babel-core/register \"src/**/*spec.js\"",
    "update-theme": "node update-theme.js",
    "shoutem": "node build/src/shoutem.js",
    "push-and-publish": "npm run update-theme && git push && npm publish --access public",
    "postinstall": "node build/src/scripts/postinstall.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shoutem/platform-build": "0.0.5",
    "async": "2.0.0-rc.3",
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.6",
    "command-exists": "^1.0.2",
    "download": "^5.0.2",
    "fs-extra": "^1.0.0",
    "inquirer": "1.0.3",
    "lodash": "4.11.2",
    "mkdirp": "0.5.1",
    "mkdirp-promise": "^3.0.1",
    "mustache": "2.2.1",
    "mz": "^2.4.0",
    "request": "^2.72.0",
    "request-promise": "^4.1.1",
    "request-promise-native": "^1.0.3",
    "rimraf": "2.5.2",
    "rmfr": "^1.0.1",
    "semver": "^5.3.0",
    "superagent": "^3.0.0",
    "superagent-jsonapify": "^1.4.2",
    "superspawn": "^0.1.0",
    "tar.gz": "^1.0.5",
    "tmp-promise": "^1.0.2",
    "underscore": "^1.8.3",
    "urijs": "1.18.0",
    "yargonaut": "^1.1.2",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-rewire": "1.0.0-rc-2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "chai": "3.5.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-mocha": "2.2.0",
    "eslint-plugin-react": "5.2.2",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "rimraf": "2.5.2",
    "sinon": "1.17.4"
  }
}
